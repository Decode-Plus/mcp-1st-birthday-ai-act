    "@ai-sdk/openai": "^2.0.74",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "ai": "^5.0.104",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.5",
    "@types/node": "^22.10.2",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "eu-ai-act",
    "compliance",
    "ai-agent",
    "gradio",
